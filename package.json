{
  "name": "blog",
  "version": "0.0.1",
  "scripts": {
    "dev": "node GenerateJsonData.js && svelte-kit dev --host",
    "build": "node GenerateJsonData.js && rm -rf build && svelte-kit build && touch build/.nojekyll",
    "deploy": "npm run build && npx gh-pages -d build -t true",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@codewithshin/svelte-utterances": "^0.1.2",
    "@iconify/json": "^2.1.6",
    "@jsdevtools/rehype-toc": "^3.0.2",
    "@sveltejs/adapter-static": "^1.0.0-next.28",
    "@sveltejs/kit": "next",
    "@types/cookie": "^0.4.1",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "babel-loader": "^8.2.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "gh-pages": "^3.2.3",
    "lowdb": "^3.0.0",
    "mdsvex": "^0.10.5",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-external-links": "^1.0.1",
    "rehype-katex-svelte": "^1.1.1",
    "rehype-slug": "^5.0.1",
    "remark-math": "^3.0.0",
    "sass": "^1.49.7",
    "svelte": "^3.46.4",
    "svelte-check": "^2.2.6",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.10.1",
    "svelte-sitemap": "^2.3.0",
    "svelte-typewriter": "^3.0.0-alpha.11",
    "tslib": "^2.3.1",
    "typescript": "~4.5.4",
    "unplugin-icons": "^0.13.2"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/fira-mono": "^4.5.0",
    "@fontsource/open-sans": "^4.5.5",
    "@fontsource/roboto": "^4.5.3",
    "@lukeed/uuid": "^2.0.0",
    "cookie": "^0.4.1",
    "dayjs": "^1.10.7",
    "katex": "^0.15.2",
    "svelte-particles": "^1.41.4"
  }
}
